<h5>Таблица</h5>
<div class="row">
    <div class="col s3">
        <label for="studentsName">Имя студента</label>
        <h6 name="studentsName"><a href="/journal/watchByStudent/{{answer.userId._id}}">{{answer.userId.name}}</a></h6>
    </div>
    <div class="col s9">
        <label for="askText">Задание</label>
        <h6 name="askText">{{answer.ask.askText}}</h6>
    </div>
    
</div>
<div class="col s6">
    <table class="table-for-ask">
        <tbody>
            {{#each answer.tableAnswer as | row |}}
            <tr>
                {{#each row as | column |}}
                <td>
                    {{column}}
                </td>
                {{/each}}
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>

<div style="margin-top: 10px">
    <a href="/journal/watchBySubTask/{{answer.ask.subTaskId}}">
        <button class="btn waves-effect waves-light">Назад к теме</button>
    </a>
</div>